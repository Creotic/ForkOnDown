<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="veliden.forkondown.MainActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/sConsole"
        android:layout_width="178dp"
        android:layout_height="166dp"
        android:layout_marginBottom="17dp"
        android:layout_marginTop="8dp"
        android:gravity="bottom"
        android:scrollbars="vertical"
        android:textAlignment="textStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.068"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.895"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/btnLeft"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:text="◄"
        android:visibility="invisible"
        app:layout_constraintRight_toLeftOf="@+id/btnFwd"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintBaseline_toBaselineOf="@+id/grid_21"
        tools:layout_constraintBaseline_creator="1"
        android:layout_marginRight="-14dp" />

    <Button
        android:id="@+id/btnRight"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:text="►"
        android:visibility="invisible"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/btnFwd"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/btnFwd" />

    <Button
        android:id="@+id/btnFwd"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:text="▲"
        android:visibility="invisible"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/btnLeft"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/btnBwd"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:text="▼"
        android:visibility="invisible"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/grid_22"
        app:layout_constraintTop_toBottomOf="@+id/grid_22"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/grid_22" />

    <ToggleButton
        android:id="@+id/btnToggle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:text="ToggleButton"
        android:textOff="Auto"
        android:textOn="Manual"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/grid_31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3,1"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/btnToggle"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/btnToggle"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/btnToggle" />

    <Button
        android:id="@+id/grid_32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3,2"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/btnFwd"
        android:layout_marginStart="1dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="29dp"
        app:layout_constraintLeft_toLeftOf="@+id/btnFwd" />

    <Button
        android:id="@+id/grid_33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3,3"
        android:layout_marginEnd="16dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/btnFwd"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="77dp"
        android:layout_marginBottom="77dp"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/grid_11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1,1"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/grid_21"
        android:layout_marginTop="75dp"
        app:layout_constraintTop_toBottomOf="@+id/grid_21"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/grid_21" />

    <Button
        android:id="@+id/grid_12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1,2"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/btnBwd"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/btnBwd"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/btnBwd" />

    <Button
        android:id="@+id/grid_13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:text="1,3"
        app:layout_constraintBottom_toBottomOf="@+id/grid_12"
        app:layout_constraintEnd_toEndOf="@+id/grid_23"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="@+id/grid_23"
        app:layout_constraintTop_toTopOf="@+id/grid_12"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1" />

    <Button
        android:id="@+id/grid_21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2,1"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/grid_31"
        android:layout_marginTop="77dp"
        app:layout_constraintTop_toBottomOf="@+id/grid_31"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/grid_31"
        app:layout_constraintHorizontal_bias="1.0" />

    <Button
        android:id="@+id/grid_22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2,2"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/btnFwd"
        app:layout_constraintTop_toBottomOf="@+id/btnFwd"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/btnFwd" />

    <Button
        android:id="@+id/grid_23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2,3"
        android:layout_marginEnd="16dp"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/btnRight"
        tools:layout_constraintBaseline_creator="1" />

    <TextView
        android:id="@+id/sView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Sending"
        android:textAllCaps="true"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/sConsole"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/grid_11"
        app:layout_constraintRight_toRightOf="@+id/grid_11"
        app:layout_constraintTop_toBottomOf="@+id/grid_11"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnRaise"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Raise Fork"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/btnLower"
        app:layout_constraintEnd_toEndOf="@+id/grid_21"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/grid_21"
        app:layout_constraintTop_toTopOf="@+id/btnLower"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btnLower"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:text="Lower Fork"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/grid_31"
        app:layout_constraintEnd_toEndOf="@+id/grid_23"
        app:layout_constraintHorizontal_bias="0.724"
        app:layout_constraintStart_toStartOf="@+id/grid_23"
        app:layout_constraintTop_toTopOf="@+id/grid_31"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Home"
        app:layout_constraintBottom_toBottomOf="@+id/dock"
        app:layout_constraintEnd_toEndOf="@+id/grid_32"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/grid_32"
        app:layout_constraintTop_toTopOf="@+id/dock"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/dock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:text="Dock"
        app:layout_constraintBottom_toBottomOf="@+id/btnToggle"
        app:layout_constraintEnd_toEndOf="@+id/grid_23"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/grid_23"
        app:layout_constraintTop_toTopOf="@+id/btnToggle"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/sClear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Clear"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.026"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/rConsole"
        android:layout_width="178dp"
        android:layout_height="166dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:gravity="bottom"
        android:scrollbars="vertical"
        android:textAlignment="textEnd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.963"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.876" />

    <TextView
        android:id="@+id/rView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="Receiving"
        android:textAllCaps="true"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/sView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.975"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/sView"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/rClear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Clear"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.973"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

</android.support.constraint.ConstraintLayout>
